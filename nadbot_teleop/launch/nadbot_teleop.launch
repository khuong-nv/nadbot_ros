<launch>
  <node pkg="nadbot_teleop" type="nadbot_teleop_key.py" name="nadbot_teleop_keyboard"  output="screen">
    <param name="scale_linear" value="0.5" type="double"/>
    <param name="scale_angular" value="1.5" type="double"/>
    <remap from="nadbot_teleop_keyboard/cmd_vel" to="cmd_vel"/>
  </node>
</launch>
